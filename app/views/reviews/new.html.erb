<!-- simple_form_for([]) or simple_form_for [] both work(second:there's a space between for and []) -->

<%= simple_form_for [@restaurant, @review] do |f| %>
  <%= f.input :content %>
  <%= f.input :rating %>
  <%= f.submit "add a review", class: "btn btn-primary" %>
<% end %>
<%= link_to "Back", restaurant_path(@restaurant) %>
